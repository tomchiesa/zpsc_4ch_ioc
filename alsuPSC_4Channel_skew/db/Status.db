#Rx Connection PV
record(bi, "$(PriSys){$(PSC)}Rxconn") {
  #field(DTYP, "PSC Ctrl Connected")
  field(DTYP, "PSC Reg")
  #field(INP , "@StatusPort_$(PSC)")
  field(INP , "@StatusPort_$(PSC) 64")
  field(SCAN, "I/O Intr")
  field(ZNAM, "Disconnected")
  field(ONAM, "Connected")
  field(ZSV , "MAJOR")
}

